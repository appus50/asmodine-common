{
    "name": "asmodine/common",
    "type": "symfony-bundle",
    "version": "0.1.0",
    "description": "Asmodine Library",
    "homepage": "https://www.asmodine.fr",
    "license": "proprietary",
    "authors": [
        {
            "name": "Asmodine",
            "homepage": "https://www.asmodine.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "cocur/slugify": "^3.1",
        "kriswallsmith/buzz": "^1.0",
        "nyholm/psr7": "^1.1",
        "symfony/config": "^4.3",
        "symfony/console": "^4.3",
        "symfony/dependency-injection": "^4.3",
        "symfony/dotenv": "^4.3",
        "symfony/flex": "*",
        "symfony/http-kernel": "^4.3",
        "symfony/monolog-bundle": "^3.3",
        "symfony/orm-pack": "*",
        "symfony/translation": "^4.3",
        "symfony/workflow": "^4.3",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "friendsoftwig/twigcs": "^3.1",
        "pdepend/pdepend": "^2.5",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd": "^2.6",
        "phpmetrics/phpmetrics": "^2.4",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/phpunit-bridge": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "bin-dir": "bin/",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Asmodine\\CommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Asmodine\\CommonBundle\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": null
    }
}
